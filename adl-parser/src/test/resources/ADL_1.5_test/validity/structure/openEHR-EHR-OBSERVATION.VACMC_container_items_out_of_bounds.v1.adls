description
	original_author = <
		["name"] = <"Thomas Beale">
		["organisation"] = <"Ocean Informatics">
		["date"] = <"05/06/2011">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"Test VACMC error, triggered by occurrences of container attributes being outside of cardinality bounds.">
			copyright = <"copyright 2011 openEHR Foundation">
		>
	>
	lifecycle_state = <"AuthorDraft">
	other_details = <
		["regression"] = <"VACMC">
	>

definition
	OBSERVATION[at0000] matches {	
		data matches {
			HISTORY[at0001] matches {	
				events cardinality matches {1; unordered} matches {
					EVENT[at0002] matches {	
						data matches {
							ITEM_TREE[at0003] matches {	
								items matches {
									CLUSTER[at0004] matches {	
										items cardinality matches {1; unordered} matches {
											ELEMENT[at0005] occurrences matches {0..1} matches {*}
											ELEMENT[at0006] occurrences matches {0..1} matches {*}
										}
									}
									CLUSTER[at0007] matches {	
										items cardinality matches {1..2} matches {
											ELEMENT[at0008] occurrences matches {1} matches {*}
											ELEMENT[at0009] occurrences matches {0..1} matches {*}
											ELEMENT[at0010] occurrences matches {0..1} matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

